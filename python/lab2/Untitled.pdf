   ],
   "source": [
    "#print element after removing even number\n",
    "n=int(input(\"ENTER n: \"))\n",
    "list =[]\n",
    "result=[]\n",
    "for i in range(n):\n",
    "    num = int(input(\"enter  element :\"))\n",
    "    list.append(num)\n",
    "print(list)\n",
    "    \n",
    "for i in list:\n",
