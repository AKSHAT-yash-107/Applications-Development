   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "ENTER n:  8\n",
      "enter  element : 1\n",
      "enter  element : 2\n",
      "enter  element : 3\n",
      "enter  element : 2\n",
      "enter  element : 3\n",
      "enter  element : 1\n",
      "enter  element : 5\n",
      "enter  element : 7\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 2, 3, 2, 3, 1, 5, 7]\n",
      "list after removing dups = \n",
      "[1, 2, 3, 5, 7]\n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"ENTER n: \"))\n",
    "list =[]\n",
    "result=[]\n",
    "for i in range(n):\n",
    "    num = int(input(\"enter  element :\"))\n",
    "    list.append(num)\n",
    "    \n",
    "print(list)\n",
    "\n",
    "for i in list:\n",
    "    if i  not in result:\n",
    "        result.append(i)\n",
    "\n",
    "print(\"list after removing dups = \")\n",
    "print(result)\n",
    "\n",
    "\n",
    "        "
