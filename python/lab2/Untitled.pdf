    "# 3.>  Create three lists of numbers, their squares and cubes"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "cc398c33-f7dc-452d-bd5c-92a24c5ca0d2",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "ENTER n:  5\n",
      "enter  element : 1\n",
      "enter  element : 2\n",
      "enter  element : 3\n",
      "enter  element : 4\n",
      "enter  element : 5\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "list= [1, 2, 3, 4, 5]\n",
      "squared list= [1, 4, 9, 16, 25]\n",
      "cubed list= [1, 8, 27, 64, 125]\n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"ENTER n: \"))\n",
    "list =[]\n",
    "square=[]\n",
    "cube=[]\n",
    "for i in range(n):\n",
    "    num = int(input(\"enter  element :\"))\n",
    "    list.append(num)\n",
    "    \n",
    "\n",
    "\n",
    "for i in list:\n",
    "    square.append(i**2)\n",
    "for i in list:\n",
    "    cube.append(i**3)\n",
    "\n",
    "\n",
    "print(\"list=\",list)\n",
    "print(\"squared list=\",square)\n",
    "print(\"cubed list=\",cube)\n",
    "\n",
    "\n",
    "\n",
    "    \n"
   ]
  },
