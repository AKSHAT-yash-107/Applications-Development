    "# 2.>Python count vowels in a string"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "id": "d526a7cf-1706-4509-afd9-00566ceb07fb",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a string:  akshat shrivastava\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Total vowels: 6\n",
      "Individual count: {'a': 5, 'e': 0, 'i': 1, 'o': 0, 'u': 0}\n"
     ]
    }
   ],
   "source": [
    "s = input(\"Enter a string: \").lower()\n",
    "vowels = {'a':0, 'e':0, 'i':0, 'o':0, 'u':0}\n",
    "\n",
    "for ch in s:\n",
    "    if ch in vowels:\n",
    "        vowels[ch] += 1\n",
    "\n",
    "print(\"Total vowels:\", sum(vowels.values()))\n",
    "print(\"Individual count:\", vowels)\n"
   ]
